<div class="container text-center p-5">
  <div class="row justify-content-center">
    <div class="col-4">
      <h1>All The Guests</h1>

      <ul class="list-group">
        <% @guests.each do |guest| %>
        <li class="list-group-item"><%= link_to guest.name, guest_path(guest) %></li>
        <% end %>
      </ul>
    </div>
  </div>
</div>